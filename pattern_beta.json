{
  "mimeLayout": "mixed",
  "mimeLayoutPool": [],
  "encoding": "8bit",
  "boundaryStyle": "default",
  "boundaryPrefix": "----boundary_",
  "boundaryHexLen": 24,

  "subjectPrefix": "",
  "subjectCase": "as-is",
  "subjectMode": "user",
  "subjectJson": ["#FILENAME#"],

  "fromFormat": "as-is",
  "fromNameMode": "perEmail",

  "timing": {
    "perEmailDelayMs": { "min": 0, "max": 0 },
