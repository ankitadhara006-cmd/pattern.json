  "openTracking": false,
  "plainHtmlOrder": "fixed",

  "listUnsubscribeMode": "none",

  "messageIdMode": "off",
  "messageIdDomain": "gmail.com",

  "dateMode": "off",
  "dateFixed": "2025-09-26T13:45:00Z",
  "dateOffsetSeconds": 0,
  "dateFormat": "r",

  "sendFormat": "plain",

  "pdfMetadata": {
    "mode": "strip",
   },

  "parallelism": 3
